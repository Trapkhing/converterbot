{
  "version": 2,
  "functions": {
    "api/webhook.py": {
      "runtime": "python3.11",
      "handler": "__handler__"
    },
    "api/setup_webhook.py": {
      "runtime": "python3.11",
      "handler": "__handler__"
    }
  },
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/api/setup-webhook",
      "methods": ["GET"],
      "dest": "api/setup_webhook.py"
    }
  ]
}
